CREATE TABLE users(
id INT NOT NULL AUTO_INCREMENT,
sex INT NOT NULL,
username VARCHAR  (16) NOT NULL,
password VARCHAR  (32) NOT NULL,
email VARCHAR (32) NOT NULL,
withMeets VARCHAR (32) NOT NULL,
targetMeet VARCHAR (32) NOT NULL,
targetHeight VARCHAR (32) NOT NULL,
targetFat VARCHAR (32) NOT NULL,
birthDay VARCHAR (32) NOT NULL,
liked VARCHAR (255),
aboutUser VARCHAR (999),
linkToIMG VARCHAR (9999),
PRIMARY KEY (id)
);


CREATE TABLE PO_List(
id INT NOT NULL AUTO_INCREMENT,
name VARCHAR  (16) NOT NULL,
Platform VARCHAR  (16) NOT NULL,
releaseDate VARCHAR  (32) NOT NULL,
arch VARCHAR (32) NOT NULL,
Creator VARCHAR (32) NOT NULL,
License VARCHAR (32) NOT NULL,
Version VARCHAR (32) NOT NULL,
Cost INT NOT NULL,
urlToImg VARCHAR (32) NOT NULL,
PRIMARY KEY (id)
);


CREATE TABLE messages(
mes_id
from INT,
to INT,
text VARCHAR (99999)
);